{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "repositories": [
        {
            "type": "path",
            "url": "./packages/*"
        }
    ],
    "require": {
        "php": "^8.1",
        "alexjustesen/filament-spatie-laravel-activitylog": "^0.6.3",
        "aymanalhattami/filament-page-with-sidebar": "^1.0",
        "barryvdh/laravel-debugbar": "^3.8",
        "bezhansalleh/filament-language-switch": "^1.0",
        "bezhansalleh/filament-shield": "^2.4",
        "doctrine/dbal": "^3.6",
        "fakerphp/faker": "^1.22",
        "filament/filament": "^2.17",
        "filament/spatie-laravel-media-library-plugin": "^2.17",
        "guzzlehttp/guzzle": "^7.2",
        "laravel-frontend-presets/tall": "^6.2",
        "laravel/framework": "^10.10",
        "laravel/sail": "^1.18",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "livewire/livewire": "^2.12",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.5.2",
        "pestphp/pest": "^2.6",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpunit/phpunit": "^10.1",
        "rezaamini-ir/laravel-easyblade": "dev-master",
        "spatie/laravel-html": "^3.2",
        "spatie/laravel-ignition": "^2.0",
        "spatie/laravel-permission": "^5.4",
        "stijnvanouplines/blade-country-flags": "^1.0",
        "symfony/thanks": "^1.2",
        "pboivin/filament-peek": "^0.2.1",
        "solution-forest/filament-tree": "^1.2"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.10",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "resources/functions/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "code-qualit-test": [
            "@php ./vendor/laravel/pint/builds/pint --config=./pint.json -v --test"
        ],
        "pint:fix": "@php ./vendor/laravel/pint/builds/pint --config=./pint.json -v",
        "pint:test": "@php ./vendor/laravel/pint/builds/pint --config=./pint.json -v --test",
        "code:lint:check": [
            "pint:test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "symfony/thanks": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
